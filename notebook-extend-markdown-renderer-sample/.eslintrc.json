{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "ignorePatterns": [
        "webpack.config.js"
    ],
    "rules": {
        "@typescript-eslint/semi": "warn",
        "semi": "off",
        "@typescript-eslint/no-var-requires": 0
    }
}