{
	"compilerOptions": {
		"rootDir": "./src",
		"skipLibCheck": true,
		"lib": [
			"ES2024",
			"webworker"
		],
		"types": [
			"vscode",
			"node"
		],
		"module": "Node16",
		"moduleResolution": "Node16",
		"outDir": "./out",
		"strict": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"declaration": true,
		"stripInternal": true,
		"sourceMap": true,
		"declarationMap": true,
		"noUnusedLocals": false,
		"noUnusedParameters": false
	}
}