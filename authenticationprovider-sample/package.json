{
  "name": "vscode-authenticationprovider-sample",
  "displayName": "AuthenticationProvider API Sample",
  "description": "AuthenticationProvider API Sample",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-authenticationprovider-sample.login",
        "title": "Login with Azure DevOps"
      }
    ],
    "authentication": [
      {
        "id": "azuredevopspat",
        "label": "Azure DevOps PAT"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/node": "^16.18.34",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "glob": "^7.1.7",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "isomorphic-fetch": "^3.0.0"
  }
}
